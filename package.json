{
  "name": "@openformat/subgraph",
  "version": "0.0.1",
  "scripts": {
    "reinstall": "rm -rf .pnp* && yarn",
    "clean": "rm -rf build/ generated/",
    "prepare:mumbai": "mustache networks/mumbai.json subgraph.template.yaml subgraph.mumbai.yaml",
    "prepare:polygon": "mustache networks/polygon.json subgraph.template.yaml subgraph.polygon.yaml",
    "prepare:local": "mustache networks/local.json subgraph.template.yaml subgraph.local.yaml",
    "prepare:aurora": "mustache networks/aurora.json subgraph.template.yaml subgraph.aurora.yaml",
    "prepare:aurora-testnet": "mustache networks/aurora-testnet.json subgraph.template.yaml subgraph.aurora-testnet.yaml",
    "create:local": "graph create open-format/metrics --node http://0.0.0.0:8020",
    "create:mumbai": "graph create open-format/metrics-mumbai --node https://api.thegraph.com/deploy/",
    "create:polygon": "graph create open-format/metrics-polygon --node https://api.thegraph.com/deploy/",
    "create:aurora": "graph create open-format/metrics-aurora --node https://api.thegraph.com/deploy/",
    "create:aurora-testnet": "graph create open-format/metrics-aurora-testnet --node https://api.thegraph.com/deploy/",
    "gen:mumbai": "graph codegen subgraph.mumbai.yaml",
    "gen:polygon": "graph codegen subgraph.polygon.yaml",
    "gen:aurora": "graph codegen subgraph.aurora.yaml",
    "gen:aurora-testnet": "graph codegen subgraph.aurora-testnet.yaml",
    "gen:local": "graph codegen subgraph.local.yaml",
    "deploy:local": "graph deploy open-format/metrics --ipfs http://0.0.0.0:5001 --node http://0.0.0.0:8020 subgraph.local.yaml",
    "deploy:mumbai": "graph deploy --product hosted-service open-format/metrics-mumbai subgraph.mumbai.yaml",
    "deploy:aurora": "graph deploy --product hosted-service open-format/metrics-aurora subgraph.aurora.yaml",
    "deploy:aurora-testnet": "graph deploy --product hosted-service open-format/metrics-aurora-testnet subgraph.aurora-testnet.yaml",
    "deploy:polygon": "graph deploy --product hosted-service open-format/metrics-polygon subgraph.polygon.yaml",
    "build": "graph build",
    "auth": "graph auth https://api.thegraph.com/deploy/"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@graphprotocol/graph-cli": "^0.61.0",
    "@graphprotocol/graph-ts": "^0.29.3",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.3.0",
    "mustache": "^4.2.0"
  },
  "packageManager": "yarn@1.22.1"
}
